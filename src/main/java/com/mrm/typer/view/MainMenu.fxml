<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" maxHeight="800.0" maxWidth="1000.0" minHeight="800.0" minWidth="1000.0" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mrm.typer.controller.MainMenuController">
    <children>
        <AnchorPane fx:id="mainPane" maxHeight="700.0" maxWidth="1000.0" minHeight="700.0" minWidth="1000.0" prefHeight="700.0" prefWidth="1000.0">
            <children>
                <Button fx:id="btn_NewGame" layoutX="454.0" layoutY="288.0" mnemonicParsing="false" onMouseClicked="#action_NewGame" text="New Game" />
                <Button fx:id="btn_Scores" layoutX="466.0" layoutY="321.0" mnemonicParsing="false" onMouseClicked="#action_Scores" text="Scores" />
                <Button fx:id="btn_Exit" layoutX="474.0" layoutY="352.0" mnemonicParsing="false" onMouseClicked="#action_Exit" text="Exit" />
                <Label fx:id="label_name" layoutX="277.0" layoutY="177.0" text="Name:">
                    <font>
                        <Font size="34.0" />
                    </font>
                </Label>
                <TextField fx:id="textfield_name" layoutX="385.0" layoutY="165.0" promptText="yourname">
                    <font>
                        <Font size="34.0" />
                    </font>
                </TextField>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="subPane" layoutY="700.0" maxHeight="100.0" maxWidth="1000.0" minHeight="100.0" minWidth="1000.0" prefHeight="100.0" prefWidth="1000.0">
         <children>
            <Label fx:id="Welcome" layoutX="335.0" layoutY="-25.0" prefHeight="111.0" prefWidth="330.0" text="Welcome" textAlignment="CENTER">
               <font>
                  <Font size="68.0" />
               </font>
            </Label>
         </children>
        </AnchorPane>
    </children>
</AnchorPane>
